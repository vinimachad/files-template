{
  "name": "filestemplate",
  "displayName": "FilesTemplate",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:filestemplate.createFileTemplates",
    "onCommand:filestemplate.createVIPSceneStateLess"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "filestemplate.createFileTemplates",
        "title": "ðŸŸ¢ Create files from template"
      },
      {
        "command": "filestemplate.createVIPSceneStateLess",
        "title": "ðŸ”µ Create flutter scene with state less"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "filestemplate.createFileTemplates",
          "group": "0a_modification",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "filestemplate.createVIPSceneStateLess",
          "group": "0a_modification",
          "when": "explorerResourceIsFolder"
        }
      ]
    },
    "configuration": {
      "title": "File template tools",
      "properties": {
        "filestemplate.fileExtension": {
          "type": "string",
          "markdownDescription": "Default file extension for Flutter files."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/js-yaml": "^4.0.5",
    "@types/mocha": "^10.0.1",
    "@types/mustache": "^4.2.2",
    "@types/node": "16.x",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vscode/test-electron": "^2.2.2",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "mustache": "^4.2.0"
  }
}
